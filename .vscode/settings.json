{
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true
  },
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer",
    "editor.formatOnSave": true,
  },
  "deno.enable": true,
  "deno.lint": true
}